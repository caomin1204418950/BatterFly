.header-right {
  position: absolute;
  right: 30px;
  top: 14px;
}
.form-group-btn {
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-align: center;
  -ms-flex-align: center;
  align-items: center;
  -webkit-box-pack: end;
  -ms-flex-pack: end;
  justify-content: flex-end;
  padding: 1rem;
  padding-bottom: 0;
  margin-bottom: 0;
  border-top: 1px solid #e9ecef;
}
.form-group-btn .btn {
  margin-left: 10px;
}
.tree-box {
  height: 400px;
  width: 110%;
  overflow: hidden;
}
.checkbox {
  padding: 0 20px;
}
.checkbox .border-checkbox-group {
  cursor: pointer;
}